<h1>Users</h1>

<section class="flex flex-col gap-2">
{{#each users}}
  <article class="p-2 text-white bg-slate-600">
    <a href="/admin/users/{{ this.id }}">{{this.username}}</a>
    <a class="p-2 text-white bg-yellow-600 cursor-pointer" href="/admin/users/{{ this.id }}/delete">Delete</a>
  </article>
{{/each}}
</section>
  